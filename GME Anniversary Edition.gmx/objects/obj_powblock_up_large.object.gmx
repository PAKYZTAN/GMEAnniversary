|| (collision_rectangle(x-1,bbox_bottom+1,x+1,bbox_bottom+1,obj_slopeparent,1,0)) {

    //Stop gravity
    gravity = 0;
    
    //If the vertical speed is greater than 0.5
    if (vspeed &gt; 0) {
    
        //Bounce back
        vspeed = -3;
    
