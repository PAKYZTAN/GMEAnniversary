&amp;&amp; (!collision_rectangle(bbox_left,bbox_bottom-4,bbox_right,bbox_bottom-4,obj_semisolid,0,0))) {

    //Destroy if moving down
    if (vspeed &gt; 0)
        event_user(0);
}

//If the player is moving up
