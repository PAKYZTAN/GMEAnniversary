    if (other.object_index == obj_playerparent) {
    
        //If the player is sliding
        if (other.sliding)        
            imdead.hspeed = other.xspeed;
    }
        
    //Otherwise, set default speed
    else {
    
        if (other.hspeed &gt; 0)
            imdead.hspeed = 0.6;
        else if (other.hspeed &lt; 0)
            imdead.hspeed = -0.6;
    }
