            <string>///Finish pull

//If the puller object exists
if (instance_exists(obj_puller)) {

    //With the player
    with (obj_playerparent) {
    
        visible = 1;
        holding = 0;
        disablecontrol = false;
    }
    
    //Destroy puller
    with (obj_puller) instance_destroy();
    
    //Destroy
    instance_destroy();
