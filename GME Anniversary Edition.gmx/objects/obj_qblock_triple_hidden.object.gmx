            <string>///Make it visible

//Check for the player
var player = collision_rectangle(bbox_left,bbox_bottom,bbox_right,bbox_bottom+2,obj_playerparent,0,0);

//If the player is in position
if (player)
&amp;&amp; (player.yspeed &lt; 0)
&amp;&amp; (player.x &gt; bbox_left) 
&amp;&amp; (player.x &lt; bbox_right)
&amp;&amp; (player.bbox_top &gt; bbox_bottom+1+player.yspeed) {

    //Create a new block, give it the same item and bump it.
    with (instance_create(x,y,obj_qblock_triple)) {
    
        //Bump it
        ready = 1;
        
        //Set the vertical speed
        vspeed = -2;
        
        //Move down
        alarm[0] = 4;
        
        //Hereby the items
        sprout[0] = other.sprout[0];
        sprout[1] = other.sprout[1];
        sprout[2] = other.sprout[2];
        
        //Hereby block specific events
        event_user(0);
    }
    
    //Destroy
    instance_destroy();
}
