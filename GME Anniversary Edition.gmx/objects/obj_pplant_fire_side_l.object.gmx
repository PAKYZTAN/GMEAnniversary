if (instance_exists(obj_playerparent)) {

    //If the player is above the plant
    if (obj_playerparent.y &lt; y+8) {
    
        //If there's a fireball overlapping
        if (collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_bottom,obj_plantfire,1,0))
            image_index = 3;
        else
            image_index = 2;
    }
    
    //Otherwise, if the player is below the plant
    else if (obj_playerparent.y &gt; y+8) {
    
        //If there's a fireball overlapping
        if (collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_bottom,obj_plantfire,1,0))
            image_index = 1;
        else
            image_index = 0;
    }
}
else {

    //If there's a fireball overlapping
    if (place_meeting(x,y,obj_plantfire))
        image_index = 3;
    else
        image_index = 2;    
}

//Change to ice
if (ice) then sprite_index = spr_pplant_side_ice;
