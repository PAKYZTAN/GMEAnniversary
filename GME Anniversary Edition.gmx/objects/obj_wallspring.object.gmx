            audio_play_sound(snd_trampoline, 0, false);
            
            //Set the player vertical speed
            obj_playerparent.yspeed = -4.725;
            
            //Boost Jump
            obj_playerparent.y--;
            
            //Check if 'Shift' is pressed.
            if (keyboard_check(global.shiftkey))
                obj_playerparent.jumping = 1;
            else
                obj_playerparent.jumping = 2;
        }
    }
    
    //Allow use
    ready = 0;
    alarm[0] = 8;
    
    //Restart animation
    image_speed = 1;
    image_index = 0;
    
    //Set the end sprite
    sprite_index = spr_wallspring_end;
}
else if (sprite_index == spr_wallspring_end) {

    //Do not animate
    image_speed = 0;
