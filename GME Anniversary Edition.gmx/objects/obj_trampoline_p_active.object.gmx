            //Set the player vertical speed
            obj_playerparent.yspeed = -5.425;
            
            //Boost Jump
            obj_playerparent.y--;
            
            //Check if 'Shift' is pressed.
            if (keyboard_check(global.shiftkey))
                obj_playerparent.jumping = 1;
            else
                obj_playerparent.jumping = 2;
        }
    }
    
    //Allow use
    ready = 0;
    alarm[0] = 8;
    
