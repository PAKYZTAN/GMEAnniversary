          <argument>
            <kind>1</kind>
            <string>///Throw an egg

//Before throwing an egg, check if the player exists
if (instance_exists(obj_playerparent)) {

    //If there's 6 or less green lakitu eggs in place and the player is in the given coordinates.
    if (instance_number(obj_lakitu_egg) &lt; 6) 
    &amp;&amp; (obj_playerparent.x &gt; xmin)
    &amp;&amp; (obj_playerparent.x &lt; xmax) {
    
        //Play 'Hammer' sound
        audio_play_sound(snd_hammer, 0, false);
    
        //Create a egg
        with (instance_create(x,y-16,obj_lakitu_egg)) {
        
            //Set the vertical speed
            vspeed = -4;
            
            //Set the horizontal speed
            if (obj_playerparent.x &lt; x)
                hspeed = -1;
            else
                hspeed = 1;
        }
        
        //Set throwing frame
        image_speed = 0;
        image_index = 1;

        //Set default frame
        alarm[1] = 30;
    }
    
    //Otherwise, wait
    else {
    
        alarm[0] = 150;
        exit;
    }
}

//Otherwise, wait
else {

    alarm[0] = 150;
    exit;
}
</string>
