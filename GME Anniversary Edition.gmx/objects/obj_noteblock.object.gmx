**      cs_3up
**      cs_poison
**      cs_pwing
**      cs_star
**      cs_key
**      cs_pswitch
**      cs_gswitch
**      cs_trampoline
**      cs_propellerblock
**      cs_beanstalk
**      cs_billy
*/

//Default value
sprout = cs_coin;

//Animate
image_speed = 0.15;

//Whether this block can be hit
ready = 0;

//Make it solid
mysolid = instance_create(x,y,obj_solid);
</string>
