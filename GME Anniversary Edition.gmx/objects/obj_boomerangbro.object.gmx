        with (instance_create(x+10,y-8,obj_boomerangbro_boomerang)) {
        
            hspeed = -2;
            image_xscale = -1;
            parent = other.id;
            if (other.id.blue)
                sprite_index = spr_boomerangbro_boomerang_blue
        }
    }
}

