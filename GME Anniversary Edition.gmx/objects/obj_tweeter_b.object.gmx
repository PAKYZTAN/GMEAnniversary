            <string>///Tweeter logic

//Inherit event
event_inherited();

//If there's no gravity
if (gravity == 0) {

    //If the tweeter did less than 3 jumps
    if (jumping &lt; 3) {
    
        //Jump a little
        vspeed = -1.25;
        y--;
        
        //Increment jump
        jumping++;
        
        //Animate
        image_speed = 0.1;
    }
    
    //Otherwise, if the tweeter jumped 3 times
    else {
    
        //Jump a little
        vspeed = -2.5;
        y--;
        
        //Increment jump
        jumping = 0;
        
        //Animate
        image_speed = 0.2;
    }
}

//Face towards direction
if (hspeed &gt; 0)
    xscale = 1;
else if (hspeed &lt; 0)
    xscale = -1;
